<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<Programs>
		
		<!--解密工具安装程序配置-->
		<Program>
			<!--配置名称 与外层的UsingProgramName对应-->
			<ProgramName>Xthk.CoursewareDecryptionTool</ProgramName>
			<!--默认安装路径 当有{0}是默认目录为用户的系统目录LocalApplicationData，可自定义-->
			<DefaultInstallPath>D:\Xthk\XthkDecryptionTool</DefaultInstallPath>
			<!--程序名称-->
			<DisplayName>xx系统</DisplayName>
			<!--发布者-->
			<Publisher>xx有限公司</Publisher>
			<!--应用程序EXE名称-->
			<AppExeName>Xthk.CoursewareDecryptionTool.exe</AppExeName>
			<!--应用程序进程名称（覆盖安装和卸载用）-->
			<AppProcessName>Xthk.CoursewareDecryptionTool</AppProcessName>
			<!--卸载程序EXE名称-->
			<UninstallName>Uninstall.exe</UninstallName>
			<!--主目录（当路径为系统盘（C:/）时加一个子目录）-->
			<HomeDirectory>Xthk</HomeDirectory>
			<!--开始菜单快捷方式文件夹-->
			<MenuFolder>\XthkDecryptionTool\</MenuFolder>
			<!--快捷方式名称-->
			<ShortcutName>xx系统.lnk</ShortcutName>
			<!--快捷方式卸载名称-->
			<UninstallShortcutName>卸载xx系统.lnk</UninstallShortcutName>
			<!--安装目录中快捷图标名称-->
			<IconDirectoryPath>\IcoInstall.ico</IconDirectoryPath>
			<!--安装目录中卸载快捷图标名称-->
			<UninstallIconDirectoryPath>\IcoUninstall.ico</UninstallIconDirectoryPath>
			<!--安装的文件夹-->
			<InstallFolderName>XthkDecryptionTool</InstallFolderName>
			<!--版本号-->
			<VersionNumber>0.0.0.1</VersionNumber>
			<!--启动路径-->
			<StartupUri>MainWindow.xaml</StartupUri>
		</Program>
		<!--安装程序配置-->
		
		
		<!--程序一安装程序配置-->
		<Program>
			<!--配置名称 与外层的UsingProgramName对应-->
			<ProgramName>Xthk.TeacherResearch</ProgramName>
			<!--默认安装路径 当有{0}是默认目录为用户的系统目录LocalApplicationData，可自定义-->
			<DefaultInstallPath>{0}\xx有限公司\xx系统</DefaultInstallPath>
			<!--程序名称-->
			<DisplayName>xx系统</DisplayName>
			<!--发布者-->
			<Publisher>xx有限公司</Publisher>
			<!--应用程序名称-->
			<AppExeName>Xthk.TeacherResearch.exe</AppExeName>
			<!--应用程序进程名称（覆盖安装和卸载用）-->
			<AppProcessName>Xthk.TeacherResearch</AppProcessName>
			<!--卸载程序名称-->
			<UninstallName>Uninstall.exe</UninstallName>
			<!--主目录（当路径为系统盘（C:/）时加一个子目录）-->
			<HomeDirectory>Xthk</HomeDirectory>
			<!--开始菜单快捷方式文件夹-->
			<MenuFolder>\xx系统\</MenuFolder>
			<!--快捷方式名称-->
			<ShortcutName>xx系统.lnk</ShortcutName>
			<!--快捷方式卸载名称-->
			<UninstallShortcutName>卸载xx系统.lnk</UninstallShortcutName>
			<!--安装目录中快捷图标名称-->
			<IconDirectoryPath>\IcoInstall.ico</IconDirectoryPath>
			<!--安装目录中卸载快捷图标名称-->
			<UninstallIconDirectoryPath>\IcoUninstall.ico</UninstallIconDirectoryPath>
			<!--安装的文件夹 安装目录的底层文件夹-->
			<InstallFolderName>xx系统</InstallFolderName>
			<!--版本号-->
			<VersionNumber>5.1.3.0</VersionNumber>
			<!--启动路径-->
			<StartupUri>MainWindow2.xaml</StartupUri>
		</Program>
		<!--安装程序配置-->

	</Programs>
	<!--使用的安装程序配置-->
	<UsingProgramName>Xthk.CoursewareDecryptionTool</UsingProgramName>
</configuration>

